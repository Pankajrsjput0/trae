    <color name="dark_text_secondary">#B3FFFFFF</color>
    <color name="error">#F44336</color>
    <color name="gray_background">#E0E0E0</color>
    <color name="gray_divider">#BDBDBD</color>
    <color name="gray_surface">#F5F5F5</color>
    <color name="gray_surface_variant">#EEEEEE</color>
    <color name="gray_text_disabled">#9E9E9E</color>
    <color name="gray_text_primary">#212121</color>
    <color name="gray_text_secondary">#757575</color>
    <color name="info">#2196F3</color>
    <color name="light_background">#FFFFFF</color>
    <color name="light_divider">#BDBDBD</color>
    <color name="light_surface">#F5F5F5</color>
    <color name="light_surface_variant">#E1E1E1</color>
    <color name="light_text_disabled">#9E9E9E</color>
    <color name="light_text_primary">#212121</color>
