    <color name="brand_primary_light">#64B5F6</color>
    <color name="dark_background">#121212</color>
    <color name="dark_divider">#424242</color>
    <color name="dark_surface">#1E1E1E</color>
    <color name="dark_surface_variant">#2D2D2D</color>
    <color name="dark_text_disabled">#66FFFFFF</color>
